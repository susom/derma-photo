{
	"name": "Derma Avatar Photo App",

	"namespace": "Stanford\\DermaPhoto",

	"description": "Allows survey takers to take photos on their phone (or select existing files) and link the uploaded photo to the selected image map in the survey.",

	"documentation": "README.md",

	"authors": [
		{
			"name": "Andrew Martin",
			"email": "andy123@stanford.edu",
			"institution": "Stanford University"
		},
        {
            "name": "Jae Lee",
            "email": "jael@stanford.edu",
            "institution": "Stanford University"
        },
        {
            "name": "Irvin Szeto",
            "email": "irvins@stanford.edu",
            "institution": "Stanford University"
        }
	],

    "framework-version": 8,

	"permissions": [
        "redcap_module_system_enable",
        "redcap_module_project_enable",
        "redcap_module_save_configuration",
        "redcap_survey_page_top"
	],

	"enable-every-page-hooks-on-system-pages": false,

	"links": {
		"project": [

        ],
		"control-center": [

		]
	},

	"no-auth-pages": [

	],

	"project-settings": [
		{
			"key": "enable-project-debug-logging",
			"name": "<b>Enable Debug Logging</b><br><i>(optional)</i> Requires installation and configuration of emLogger",
			"required": false,
			"type": "checkbox"
		}
	],


	"system-settings": [
		{
			"key": "enable-system-debug-logging",
			"name": "<b>Enable Debug Logging (system-wide)</b><i>(optional)</i> Requires installation and configuration of emLogger",
			"required": false,
			"type": "checkbox"
		}
	],

	"compatibility": {
		"php-version-min": "",
		"php-version-max": "",
		"redcap-version-min": "",
		"redcap-version-max": ""
	}
}
